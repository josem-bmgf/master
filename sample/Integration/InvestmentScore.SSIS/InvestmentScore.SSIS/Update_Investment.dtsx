                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[toolkit].[vInvestmentPaymentByStrategyCurrent]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	 [INV ID]
	,i.[INV Title]
	,i.[INV Type]
	,i.[INV Comments]
	,i.[INV Start Date]
	,i.[INV End Date]
	,i.[INV Status]
	,i.[INV Grantee/Vendor Name] AS INV_Grantee_Vendor_Name
	,i.[INV Level of Engagement]
	,i.[INV Owner]
	,i.[INV Manager]
	,[INV Managing Team Level 1] = ISNULL(NULLIF(i.[INV Managing Sub-Team],''),i.[INV Supporting Team Level 1])
	,[INV Managing Team Level 2] = ISNULL(NULLIF(i.[INV Managing Team],''),i.[INV Supporting Team Level 2])
	,[INV Managing Team Level 3] =  ISNULL(NULLIF(i.[INV Managing Team Level 3],''),i.[INV Supporting Team Level 1])
	,[INV Managing Team Level 4] =  ISNULL(NULLIF(i.[INV Managing Team Level 4],''),i.[INV Supporting Team Level 1])
	,i.[PMT Expenditure Type]
	,ISNULL(MAX([INV Total Payout Amt]),0)  as [INV Total Payout Amt]
	,ISNULL(SUM(i.[PMT Strategy Allocation Amt]),0) as [INV Amt Paid to Date]
	,g.[OPP Description]
FROM [toolkit].[vInvestmentPaymentByStrategyCurrent] i
LEFT JOIN [toolkit].[vGrantCurrent] g on i.[INV ID] = g.[OPP Opportunity ID] AND g.[OPP Source] = 'Unison'
WHERE [PMT Expenditure Type] = 'DCE'
	AND ([INV Status] ='Active' OR
	(	[INV Status] in ('Closed','Cancelled', 'Inactive') AND 
		(DATEPART(YEAR,GETDATE()) BETWEEN DATEPART(Year, i.[INV Start Date]) AND DATEPART(Year, i.[INV End Date]))
	) )
	AND i.[PMT Status] = 'Paid'

GROUP BY	
	[INV ID]
	,i.[INV Title]
	,i.[INV Type]
	,i.[INV Comments]
	,i.[INV Start Date]
	,i.[INV End Date]
	,i.[INV Status]
	,i.[INV Grantee/Vendor Name]
	,i.[INV Level of Engagement]
	,i.[INV Owner]
	,i.[INV Manager]
	,i.[INV Managing Sub-Team]
	,i.[INV Managing Team]
	,i.[INV Managing Team Level 3]
	,i.[INV Managing Team Level 4]
    ,i.[INV Supporting Team Level 1]
	,i.[INV Supporting Team Level 2]
	,i.[PMT Expenditure Type]
	,g.[OPP Description]</property>
