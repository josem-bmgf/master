        </QueryParameters>
        <CommandText>--DECLARE @ScoringYear NVARCHAR(20) = 2016
--DECLARE @InvestmentSize NVARCHAR(100) = '&lt;All&gt;' --{&lt;All&gt;, Large}
--DECLARE @Managing_Team NVARCHAR(100) = 'Global Health'

SELECT '&lt;All&gt;' Managing_SubTeam
UNION
SELECT DISTINCT(COALESCE(NULLIF(INV_Managing_SubTeam,''),'&lt;Blank&gt;')) 
  FROM Investment_Payment (NOLOCK)
 WHERE (INV_Managing_Team = (@Managing_Team) OR (@Managing_Team) = '&lt;All&gt;' OR (@Managing_Team='&lt;Blank&gt;' AND COALESCE(NULLIF(INV_Managing_Team,''), '')=''))
   AND (@InvestmentSize = '&lt;All&gt;' OR INV_Total_Payout_Amt &gt;= 5000000)
   AND ([INV_Status] = 'Active' -- Anything Active
        OR ( [INV_Status] IN ('Closed','Cancelled','Inactive') AND YEAR([INV_END_DATE]) = YEAR(@ScoringYear))
        )
ORDER BY 1</CommandText>
      </Query>
      <Fields>
        <Field Name="Managing_SubTeam">
          <DataField>Managing_SubTeam</DataField>
          <rd:TypeName>System.String</rd:TypeName>
