    <DataSet Name="Managing_Team">
      <Query>
        <DataSourceName>Scoring</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ScoringYear">
            <Value>=Parameters!ScoringYear.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InvestmentSize">
            <Value>=Parameters!InvestmentSize.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @InvestmentSize NVARCHAR(100) = '&lt;All&gt;' --{&lt;All&gt;, Large}
--DECLARE @ScoringYear NVARCHAR(20) = 2016

SELECT '&lt;All&gt;' Managing_Team
UNION
SELECT DISTINCT COALESCE(NULLIF(INV_Managing_Team, ''), '&lt;Blank&gt;')
  FROM Investment_Payment (NOLOCK)
 WHERE (@InvestmentSize = '&lt;All&gt;' OR INV_Total_Payout_Amt &gt;= 5000000)
   AND ([INV_Status] = 'Active' -- Anything Active
        OR ( [INV_Status] IN ('Closed','Cancelled','Inactive') AND YEAR([INV_END_DATE]) = YEAR(@ScoringYear))
        )
ORDER BY 1</CommandText>
      </Query>
      <Fields>
        <Field Name="Managing_Team">
          <DataField>Managing_Team</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
