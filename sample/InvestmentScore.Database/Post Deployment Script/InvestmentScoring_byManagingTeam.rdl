        </Field>
        <Field Name="INV_Title">
          <DataField>INV_Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INV_Total_Payout_Amt">
          <DataField>INV_Total_Payout_Amt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="INV_Description">
          <DataField>INV_Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Exclude_from_Scoring">
          <DataField>Exclude_from_Scoring</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="INV_Managing_Team">
          <DataField>INV_Managing_Team</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InvestmentSizeCategory">
          <DataField>InvestmentSizeCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INV_Managing_SubTeam">
          <DataField>INV_Managing_SubTeam</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INV_Managing_Team_Level_3">
          <DataField>INV_Managing_Team_Level_3</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PMT_Strategy_Allocation_Amt">
          <DataField>PMT_Strategy_Allocation_Amt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
