    <DataSet Name="Payments">
      <Query>
        <DataSourceName>Scoring</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Managing_Team">
            <Value>=Parameters!Managing_Team.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Managing_SubTeam">
            <Value>=Parameters!Managing_SubTeam.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Managing_Team_Level_3">
            <Value>=Parameters!Managing_Team_Level_3.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InvestmentSize">
            <Value>=Parameters!InvestmentSize.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @Managing_Team [nvarchar](100) =   'Global Policy and Advocacy'
--DECLARE @Managing_SubTeam [nvarchar](100) =  'GPA Strategy Planning &amp; Management'
--DECLARE @Managing_Team_Level_3 [nvarchar](100) = '&lt;All&gt;' --'Anchor Partnerships'
--DECLARE @InvestmentSize [nvarchar](100) =  '&lt;All&gt;'

DECLARE @Scoring_Year AS NVARCHAR(100)=CASE WHEN MONTH(GETDATE())&gt;=6 THEN YEAR(GETDATE()) ELSE YEAR(GETDATE())-1 END

;WITH Excluded AS (
SELECT ID_Combined
      ,ISNULL(MAX(CAST(Exclude_from_Scoring AS INT)),0) AS Exclude_from_Scoring
FROM vInvestmentScoringReporting
GROUP BY ID_Combined)

,AnchorStatus AS (
SELECT ID_Combined
         ,CASE WHEN
		     MAX(CASE WHEN NULLIF(LTRIM(RTRIM(Objective)),'') IS NOT NULL
		       THEN 1 
			   ELSE 0
		     END) = 1 THEN 'Anchor'
		  ELSE 'Non-Anchor'
		  END AS InvestmentSizeCategory
	FROM vInvestmentScoringReporting
    GROUP BY ID_Combined
)

SELECT
       CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
            THEN CPT_GATEWAY_ID
            ELSE INV_ID
       END ID_Combined
      ,Investment_Payment.INV_Grantee_Vendor_Name
      ,Investment_Payment.INV_Title
      ,Investment_Payment.INV_Description
      ,Investment_Payment.INV_Owner
	  ,Exclude_from_Scoring
	  ,AnchorStatus.InvestmentSizeCategory
      ,CAST(Investment_Payment.INV_Start_Date AS Date) INV_Start_Date
      ,CAST(Investment_Payment.INV_End_Date AS Date) INV_End_Date
      ,MIN(ISNULL(INV_Total_Payout_Amt,0)) INV_Total_Payout_Amt
      ,SUM(PMT_Strategy_Allocation_Amt) PMT_Strategy_Allocation_Amt
	  ,INV_Managing_Team
	  ,INV_Managing_SubTeam
	  ,INV_Managing_Team_Level_3
 FROM Investment_Payment
 LEFT JOIN Excluded
   ON CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
            THEN CPT_GATEWAY_ID
            ELSE INV_ID
       END = Excluded.ID_Combined
 LEFT JOIN AnchorStatus
   ON CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
            THEN CPT_GATEWAY_ID
            ELSE INV_ID
       END = AnchorStatus.ID_Combined
 LEFT JOIN
       (
              SELECT DISTINCT ID_Combined
              FROM dbo.vInvestmentScoringReporting
              WHERE Is_Excluded=1 and Score_Year=@Scoring_Year
       ) ex 
   ON ex.ID_Combined=Investment_Payment.INV_ID

 
WHERE Exclude_from_Scoring = 0
    AND (INV_Managing_Team = (@Managing_Team) OR (@Managing_Team) = '&lt;All&gt;' OR (@Managing_Team='&lt;Blank&gt;' AND COALESCE(NULLIF(INV_Managing_Team,''), '')=''))
    AND (INV_Managing_SubTeam = (@Managing_SubTeam) OR (@Managing_SubTeam) = '&lt;All&gt;' OR (@Managing_SubTeam='&lt;Blank&gt;' AND COALESCE(NULLIF(INV_Managing_SubTeam,''), '')='')) 
    AND (INV_Managing_Team_Level_3 = (@Managing_Team_Level_3) OR (@Managing_Team_Level_3) = '&lt;All&gt;' OR (@Managing_Team_Level_3='&lt;Blank&gt;' AND COALESCE(NULLIF(INV_Managing_Team_Level_3,''), '')='')) 

AND (@InvestmentSize = '&lt;All&gt;' 
     OR (@InvestmentSize = 'Large' AND INV_Total_Payout_Amt &gt;= 5000000)
     OR (@InvestmentSize = 'Anchor' AND InvestmentSizeCategory = 'Anchor')
    )
AND (Investment_Payment.[INV_Status] = 'Active'
     OR ( Investment_Payment.[INV_Status] IN ('Closed','Cancelled','Inactive')
          AND YEAR(Investment_Payment.[INV_END_DATE]) = YEAR(CURRENT_TIMESTAMP))
         )
AND ex.ID_Combined IS NULL
 GROUP BY
      CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
            THEN CPT_GATEWAY_ID
            ELSE INV_ID
       END 
      ,Investment_Payment.INV_Grantee_Vendor_Name
      ,Investment_Payment.INV_Title
      ,Investment_Payment.INV_Description
      ,Investment_Payment.INV_Owner
	  ,Exclude_from_Scoring
	  ,AnchorStatus.InvestmentSizeCategory
      ,CAST(Investment_Payment.INV_Start_Date AS Date) 
      ,CAST(Investment_Payment.INV_End_Date AS Date)
	  ,INV_Managing_Team
	  ,INV_Managing_SubTeam
	  ,INV_Managing_Team_Level_3</CommandText>
      </Query>
      <Fields>
        <Field Name="ID_Combined">
          <DataField>ID_Combined</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INV_End_Date">
          <DataField>INV_End_Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="INV_Grantee_Vendor_Name">
