          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT '&lt;All&gt;' PMT_Strategy
UNION
SELECT DISTINCT(PMT_Strategy) FROM Investment_Payment
WHERE (PMT_Division = (@Division) OR (@Division) = '&lt;All&gt;')
AND (@InvestmentSize = '&lt;All&gt;' OR
            (CASE WHEN EXISTS
            (SELECT 1 FROM vInvestmentScoringReporting
             WHERE ID_Combined = 
        CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
                 THEN CPT_GATEWAY_ID
                  ELSE INV_ID
         END 
         AND NULLIF(LTRIM(RTRIM(Objective)),'') IS NOT NULL)
       THEN 'Anchor'
       ELSE 'Standard'
       END = @InvestmentSize
       ) 
       OR (@InvestmentSize = 'Large'
           AND INV_Total_Payout_Amt &gt;= 5000000)
       )
AND ([INV_Status] = 'Active'
     OR ( [INV_Status] IN ('Closed','Cancelled','Inactive')
          AND YEAR([INV_END_DATE]) = YEAR(CURRENT_TIMESTAMP))
         )
ORDER BY PMT_Strategy</CommandText>
      </Query>
      <Fields>
        <Field Name="PMT_Strategy">
          <DataField>PMT_Strategy</DataField>
