    <DataSet Name="Payments">
      <Query>
        <DataSourceName>Scoring</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Division">
            <Value>=Parameters!Division.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Strategy">
            <Value>=Parameters!Strategy.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Initiative">
            <Value>=Parameters!Initiative.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@InvestmentSize">
            <Value>=Parameters!InvestmentSize.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SubInitiative">
            <Value>=Parameters!SubInitiative.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @Division [nvarchar](100) = 'Global Policy and Advocacy'
--DECLARE @Strategy [nvarchar](100) = 'Program Advocacy &amp; Comms'
--DECLARE @Initiative [nvarchar](100) = '&lt;All&gt;' --'Anchor Partnerships'
--DECLARE @SubInitiative [nvarchar](100) = '&lt;All&gt;' --'Africa'
--DECLARE @InvestmentSize [nvarchar](100) =  '&lt;All&gt;'--'Anchor'

DECLARE @Scoring_Year AS NVARCHAR(100)=CASE WHEN MONTH(GETDATE())&gt;=6 THEN YEAR(GETDATE()) ELSE YEAR(GETDATE())-1 END
;WITH Excluded AS (
SELECT ID_Combined
      ,ISNULL(MAX(CAST(Exclude_from_Scoring AS INT)),0) AS Exclude_from_Scoring
FROM vInvestmentScoringReporting
GROUP BY ID_Combined)

,AnchorStatus AS (
SELECT ID_Combined
         ,CASE WHEN
		     MAX(CASE WHEN NULLIF(LTRIM(RTRIM(Objective)),'') IS NOT NULL
		       THEN 1 
			   ELSE 0
		     END) = 1 THEN 'Anchor'
		  ELSE 'Non-Anchor'
		  END AS InvestmentSizeCategory
	FROM vInvestmentScoringReporting
    GROUP BY ID_Combined
)

SELECT
       CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
            THEN CPT_GATEWAY_ID
            ELSE INV_ID
       END ID_Combined
      ,PMT_Division
      ,PMT_Strategy
      ,PMT_Initiative
	  ,PMT_SubInitiative
      ,Investment_Payment.INV_Grantee_Vendor_Name
      ,Investment_Payment.INV_Title
      ,Investment_Payment.INV_Description
      ,Investment_Payment.INV_Owner
	  ,CASE WHEN @InvestmentSize = 'Anchor' 
	        THEN CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
                      THEN CPT_GATEWAY_ID
                      ELSE INV_ID
                 END --ID_Combined
			ELSE PMT_Initiative
	   END AS PageBreakValue
	  ,Exclude_from_Scoring
	  ,AnchorStatus.InvestmentSizeCategory
      ,CAST(Investment_Payment.INV_Start_Date AS Date) INV_Start_Date
      ,CAST(Investment_Payment.INV_End_Date AS Date) INV_End_Date
      ,MIN(ISNULL(INV_Total_Payout_Amt,0)) INV_Total_Payout_Amt
      ,SUM(PMT_Strategy_Allocation_Amt) PMT_Strategy_Allocation_Amt
 FROM Investment_Payment
 LEFT JOIN Excluded
   ON CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
            THEN CPT_GATEWAY_ID
            ELSE INV_ID
       END = Excluded.ID_Combined
 LEFT JOIN AnchorStatus
   ON CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
            THEN CPT_GATEWAY_ID
            ELSE INV_ID
       END = AnchorStatus.ID_Combined

 LEFT JOIN
       (
              SELECT DISTINCT ID_Combined
              FROM dbo.vInvestmentScoringReporting
              WHERE Is_Excluded=1 and Score_Year=@Scoring_Year
       ) ex 
   ON ex.ID_Combined=Investment_Payment.INV_ID

WHERE Exclude_from_Scoring = 0
AND  (PMT_Division = (@Division) OR (@Division) = '&lt;All&gt;')
AND (PMT_Strategy = (@Strategy) OR (@Strategy) = '&lt;All&gt;')
AND (PMT_Initiative = (@Initiative) OR (@Initiative) = '&lt;All&gt;')
AND (PMT_SubInitiative = (@SubInitiative) OR (@SubInitiative) = '&lt;All&gt;')
AND (@InvestmentSize = '&lt;All&gt;' 
     OR (@InvestmentSize = 'Large' AND INV_Total_Payout_Amt &gt;= 5000000)
     OR (@InvestmentSize = 'Anchor' AND InvestmentSizeCategory = 'Anchor')
    )
AND (Investment_Payment.[INV_Status] = 'Active'
     OR ( Investment_Payment.[INV_Status] IN ('Closed','Cancelled','Inactive')
          AND YEAR(Investment_Payment.[INV_END_DATE]) = YEAR(CURRENT_TIMESTAMP))
         )
AND ex.ID_Combined IS NULL

 GROUP BY
      CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
            THEN CPT_GATEWAY_ID
            ELSE INV_ID
       END 
      ,PMT_Division
      ,PMT_Strategy
      ,PMT_Initiative
	  ,PMT_SubInitiative
      ,Investment_Payment.INV_Grantee_Vendor_Name
      ,Investment_Payment.INV_Title
      ,Investment_Payment.INV_Description
      ,Investment_Payment.INV_Owner
	  ,Exclude_from_Scoring
	  ,AnchorStatus.InvestmentSizeCategory
      ,CAST(Investment_Payment.INV_Start_Date AS Date) 
      ,CAST(Investment_Payment.INV_End_Date AS Date)</CommandText>
      </Query>
      <Fields>
        <Field Name="ID_Combined">
          <DataField>ID_Combined</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INV_End_Date">
          <DataField>INV_End_Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="INV_Grantee_Vendor_Name">
          <DataField>INV_Grantee_Vendor_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INV_Owner">
          <DataField>INV_Owner</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INV_Start_Date">
          <DataField>INV_Start_Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="PMT_SubInitiative">
          <DataField>PMT_SubInitiative</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INV_Title">
          <DataField>INV_Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="INV_Total_Payout_Amt">
          <DataField>INV_Total_Payout_Amt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="INV_Description">
          <DataField>INV_Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PMT_Division">
          <DataField>PMT_Division</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PageBreakValue">
          <DataField>PageBreakValue</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Exclude_from_Scoring">
          <DataField>Exclude_from_Scoring</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="InvestmentSizeCategory">
          <DataField>InvestmentSizeCategory</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PMT_Initiative">
          <DataField>PMT_Initiative</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PMT_Strategy_Allocation_Amt">
          <DataField>PMT_Strategy_Allocation_Amt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PMT_Strategy">
          <DataField>PMT_Strategy</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
