        <CommandText>--DECLARE @Division [nvarchar](100) = 'Global Policy and Advocacy'
--DECLARE @Strategy [nvarchar](100) = 'Program Advocacy &amp; Comms'
--DECLARE @Initiative [nvarchar](100) = '&lt;All&gt;' --'Anchor Partnerships'
--DECLARE @InvestmentSize [nvarchar](100) =  '&lt;All&gt;'--'Anchor'

SELECT '&lt;All&gt;' PMT_SubInitiative
UNION
SELECT DISTINCT PMT_SubInitiative
FROM Investment_Payment
WHERE (PMT_Division = (@Division) OR (@Division) = '&lt;All&gt;') 
  AND (PMT_Strategy = (@Strategy) OR (@Strategy) = '&lt;All&gt;')
  AND (PMT_Initiative = (@Initiative) OR (@Initiative) = '&lt;All&gt;')
  AND (@InvestmentSize = '&lt;All&gt;' OR
            (CASE WHEN EXISTS
            (SELECT 1 FROM vInvestmentScoringReporting
             WHERE ID_Combined = 
                 CASE WHEN NULLIF(LTRIM(RTRIM(ISNULL(INV_ID,''))),'') IS NULL 
                      THEN CPT_GATEWAY_ID
                      ELSE INV_ID
                 END 
         AND NULLIF(LTRIM(RTRIM(Objective)),'') IS NOT NULL)
       THEN 'Anchor'
       ELSE 'Standard'
       END = @InvestmentSize
       ) 
       OR (@InvestmentSize = 'Large'
           AND INV_Total_Payout_Amt &gt;= 5000000))
AND ([INV_Status] = 'Active'
     OR ( [INV_Status] IN ('Closed','Cancelled','Inactive')
          AND YEAR([INV_END_DATE]) = YEAR(CURRENT_TIMESTAMP))
         )
ORDER BY PMT_SubInitiative</CommandText>
      </Query>
