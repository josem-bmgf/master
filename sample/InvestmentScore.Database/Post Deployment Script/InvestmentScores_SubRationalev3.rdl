  <DataSets>
    <DataSet Name="Score">
      <Query>
        <DataSourceName>ScoringProd</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ID_Combined">
            <Value>=Parameters!ID_Combined.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @ID_Combined NVARCHAR(20) = 'OPP1016017'
--DECLARE @InvestmentSizeTarget NVARCHAR(20) = 'Anchor'
--DECLARE @InvestmentSize NVARCHAR(20) = '&lt;All&gt;'

SELECT Objective
      ,Rationale
      ,Score_Year
      ,On_Behalf_Of
FROM
(SELECT Objective
       ,Rationale
       ,Score_Year
       ,On_Behalf_Of
       ,ROW_NUMBER() OVER(PARTITION BY ID_Combined ORDER BY Score_Year DESC) as row_number
  FROM [dbo].[vInvestmentScoringReporting]
 WHERE ID_Combined = @ID_Combined
) query
WHERE row_number = 1</CommandText>
      </Query>
      <Fields>
        <Field Name="Objective">
          <DataField>Objective</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Rationale">
          <DataField>Rationale</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Score_Year">
          <DataField>Score_Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="On_Behalf_Of">
          <DataField>On_Behalf_Of</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Title">
          <Value>=Fields!On_Behalf_Of.Value &amp; ", " &amp; Cstr(Fields!Score_Year.Value)</Value>
        </Field>
      </Fields>
    </DataSet>
