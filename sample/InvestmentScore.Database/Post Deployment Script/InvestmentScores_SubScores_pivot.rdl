  </DataSources>
  <DataSets>
    <DataSet Name="Score">
      <Query>
        <DataSourceName>ScoringProd</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="Combined">
            <Value>=Parameters!ID_Combined.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@ID_Combined">
            <Value>=Parameters!ID_Combined.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Score_Year">
            <Value>=Parameters!Score_Year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT ID_Combined
      ,Score_Year
	  ,Performance_Against_Milestones
	  ,ISNULL(Performance_Against_Strategy,'(no score)') AS Performance_Against_Strategy
	  ,ISNULL(NULLIF(Reinvestment_Prospects,''),'(no score)') AS Reinvestment_Prospects
	  ,ISNULL(Relative_Strategic_Importance,'') AS Relative_Strategic_Importance
FROM [dbo].[vInvestmentScoringReporting]
WHERE ID_Combined = @ID_Combined
  AND Score_Year &gt;= @Score_Year -2
ORDER BY Score_Year DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ID_Combined">
          <DataField>ID_Combined</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Performance_Against_Milestones">
          <DataField>Performance_Against_Milestones</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Performance_Against_Strategy">
          <DataField>Performance_Against_Strategy</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Reinvestment_Prospects">
          <DataField>Reinvestment_Prospects</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Score_Year">
          <DataField>Score_Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Relative_Strategic_Importance">
          <DataField>Relative_Strategic_Importance</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MilestonePerformance">
          <Value>=Switch(InStr(Fields!Performance_Against_Milestones.Value,"Blue") &gt; 0,"Exceeds",InStr(Fields!Performance_Against_Milestones.Value,"Red") &gt; 0,"Does Not Meet",InStr(Fields!Performance_Against_Milestones.Value,"Green") &gt; 0,"Meets",InStr(Fields!Performance_Against_Milestones.Value,"Yellow") &gt; 0,"Partially Meets",InStr(Fields!Performance_Against_Milestones.Value,"Black") &gt; 0,"Too Soon",True,"(no score)")</Value>
        </Field>
        <Field Name="StrategyPerformance">
          <Value>=Switch(IsNothing(Fields!Performance_Against_Strategy.Value),"(no score)",InStr(Fields!Performance_Against_Strategy.Value,"Fit") &gt; 0,Right(Fields!Performance_Against_Strategy.Value, Len(Fields!Performance_Against_Strategy.Value) - InStr(Fields!Performance_Against_Strategy.Value,"-")-1),InStr(Fields!Performance_Against_Strategy.Value,"Red") &gt;0,"Does Not Meet",InStr(Fields!Performance_Against_Strategy.Value,"Green")&gt;0,"Meets",InStr(Fields!Performance_Against_Strategy.Value,"Yellow")&gt;0,"Partially Meets",InStr(Fields!Performance_Against_Strategy.Value,"Blue")&gt;0,"Exceeds",InStr(Fields!Performance_Against_Strategy.Value,"Black")&gt;0,"Too Soon",
True,"(no score)")</Value>
        </Field>
        <Field Name="ReinvestmentProspects">
          <Value>=IIF(InStr(Fields!Reinvestment_Prospects.Value,"-") &gt; 0,Right(Fields!Reinvestment_Prospects.Value,Len(Fields!Reinvestment_Prospects.Value)- InStr(Fields!Reinvestment_Prospects.Value,"-")- IIF(InStr(Fields!Reinvestment_Prospects.Value,"-") &gt; 0,1,0)),Fields!Reinvestment_Prospects.Value )</Value>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!ID_Combined.Value</FilterExpression>
          <Operator>Equal</Operator>
          <FilterValues>
            <FilterValue>=Parameters!ID_Combined.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
