<environment>
  <packages>
    <package name="LEAD">
      <file name="LEADWebConfig" path="$(DEPLOYMENT_SHARE)\_PublishedWebsites\Scheduler\Web.config">
        <modifications>
          <xpath name="LEADEntities" path="/configuration/connectionStrings/add[@name='LeadershipEngagementPlannerEntities']/@connectionString">$(LEAD_ENTITIES_CONNECTIONSTRING)</xpath>
	        <xpath name="DomainLEADvalue" path = "/configuration/appSettings/add[@key='Domain']/@value">$(DomainLEAD)</xpath>
	        <xpath name="SMTPClientLEADvalue" path = "/configuration/appSettings/add[@key='SMTPClient']/@value">$(SMTPClientLEAD)</xpath>
	        <xpath name="LEADSenderMailvalue" path = "/configuration/appSettings/add[@key='LEADSender']/@value">$(LEADSenderMail)</xpath>
          <xpath name="LeadReviewerGroupvalue" path = "/configuration/appSettings/add[@key='LeadReviewerGroup']/@value">$(LeadReviewerGroupLEAD)</xpath>
          <xpath name="RequestLiasonvalue" path = "/configuration/appSettings/add[@key='RequestLiason']/@value">$(RequestLiasonLEAD)</xpath>
          <xpath name="LIPreReadsGroupvalue" path = "/configuration/appSettings/add[@key='LIPreReadsGroup']/@value">$(LIPreReadsGroupLEAD)</xpath>
          <xpath name="LeadSchedulerDomainGroupValue" path = "/configuration/appSettings/add[@key='LeadSchedulerDomainGroup']/@value">$(LeadSchedulerDomainGroupLEAD)</xpath>
	        <xpath name="DefaultEmailAddressvalue" path = "/configuration/appSettings/add[@key='DefaultEmailAddress']/@value">$(DefaultEmailAddressLEAD)</xpath>
	        <xpath name="Division_1007LEADvalue" path = "/configuration/appSettings/add[@key='Division_1007']/@value">$(Division_1007LEAD)</xpath>
	        <xpath name="Division_1001LEADvalue" path = "/configuration/appSettings/add[@key='Division_1001']/@value">$(Division_1001LEAD)</xpath>
	        <xpath name="Division_1002LEADvalue" path = "/configuration/appSettings/add[@key='Division_1002']/@value">$(Division_1002LEAD)</xpath>
	        <xpath name="Division_1003LEADsvalue" path = "/configuration/appSettings/add[@key='Division_1003']/@value">$(Division_1003LEAD)</xpath>
	        <xpath name="Division_1010LEADvalue" path = "/configuration/appSettings/add[@key='Division_1010']/@value">$(Division_1010LEAD)</xpath>
	        <xpath name="Division_1005LEADvalue" path = "/configuration/appSettings/add[@key='Division_1005']/@value">$(Division_1005LEAD)</xpath>
        </modifications>
      </file>
    </package>
  </packages>
</environment>